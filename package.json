  "scripts": {
    "start": "expo start --localhost",
    "start:lan": "expo start --lan",
    "start:tunnel": "expo start --tunnel",
    "start:localhost": "expo start --localhost",
    "start:clear": "expo start --clear --localhost",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "cd android && gradlew assembleRelease",
    "build:android:clean": "cd android && gradlew clean assembleRelease",
    "build:apk": "cd android && gradlew assembleRelease",
    "build:eas:android": "eas build --platform android --profile preview",
    "build:eas:android:prod": "eas build --platform android --profile production",
    "build:eas:run": "eas build:run -p android --latest",
    "build:eas:ios": "eas build --platform ios --profile preview",
    "build:eas:ios:prod": "eas build --platform ios --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "build:ios:testflight": "eas build --platform ios --profile production && eas submit --platform ios --latest"
  },
